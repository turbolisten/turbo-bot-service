<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.turbo.bot.base.module.wxbot.WxBotDao">

    <!-- 根据微信id查询 -->
    <select id="selectByWxId" resultType="cn.turbo.bot.base.module.wxbot.basic.domain.WxBotEntity">
        SELECT *
        FROM t_wx_bot
        WHERE bot_wx_id = #{botWxId}
    </select>
</mapper>